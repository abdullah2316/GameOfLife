<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.mygroup.ui.MainMenuController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Label fx:id="title"/>
  <Button fx:id="new_state" text="New State" />
  <Button fx:id="load_state" text="Load State" onMouseClicked="#switch_to_SavedStates"/>
  <Button fx:id="rules" text="Rules"/>
  <Button fx:id="exit" text="EXIT"/>
</VBox>
