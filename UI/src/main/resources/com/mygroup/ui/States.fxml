<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="main" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/17" fx:controller="com.mygroup.ui.StatesController">
    <HBox>
        <HBox minWidth="400" alignment="CENTER_LEFT">
            <Button fx:id="back_btn" text="Back" styleClass="statebutton" onMouseClicked="#back"/>
            <padding>
                <Insets left="20.0"/>
            </padding>
        </HBox>
        <HBox alignment="CENTER_LEFT">
            <ImageView fx:id="header">
            </ImageView>
        </HBox>

    </HBox>
    <ScrollPane fx:id="main_spane">
        <VBox fx:id="main_container" alignment="CENTER" minWidth="800" spacing="30"
              style="-fx-background-color:transparent">

            <padding>
                <Insets bottom="30" left="20.0" top="30"/>
            </padding>

        </VBox>
    </ScrollPane>
</VBox>
