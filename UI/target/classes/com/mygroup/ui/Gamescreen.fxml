<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ScrollBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Button?>
<VBox fx:id="vb" style="-fx-background-color: #6f6f6f;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mygroup.ui.GameScreenController">
    <Pane fx:id="header">
        <Label fx:id="temp" text="hellloooo" visible="false" />
        <ImageView fx:id="img" />
    </Pane>
    <BorderPane fx:id="grid_ext"  style="-fx-background-color: #404040; -fx-padding: 13px;">
      <center>
          <ScrollPane fx:id="scroller">
         <Pane fx:id="grid_internal"    style="-fx-background-color: #404040;" BorderPane.alignment="CENTER" />
          </ScrollPane>
      </center></BorderPane>
    <HBox spacing="40" >
        <Button fx:id="back_btn" text="back" onMouseClicked="#back"/>
        <Button text="Next"/>
        <Button text="Start"/>
        <Button text="Reset"/>
        <Button text="Clear"/>
    </HBox>
    <Slider fx:id="zoombar" maxWidth="100"/>
</VBox>
