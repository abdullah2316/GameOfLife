<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.image.ImageView?>
<VBox alignment="TOP_LEFT" fx:id="layout"  xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.mygroup.ui.MainMenuController">

  <Pane fx:id="header">
    <Label fx:id="temp" text="hellloooo" visible="false"/>
    <ImageView fx:id="img"/>

  </Pane>

  <VBox fx:id="menu" alignment="BASELINE_CENTER" spacing="10.0" >
  <Button fx:id="new_state" text="New State" minWidth="250"  onMouseClicked="#switch_to_gamescreen"/>
  <Button fx:id="load_state" text="Load State" minWidth="250" onMouseClicked="#switch_to_SavedStates"/>
    <Button fx:id="View_States" text="View States" minWidth="250" onMouseClicked="#switch_to_SavedStates"/>
    <Button fx:id="Delete_state" text="Delete State " minWidth="250"/>
  <Button fx:id="exit" text="EXIT" minWidth="250"/>
  </VBox>
</VBox>
